{
  "name": "dex-creator-template",
  "description": "Template for Orderly SDK",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsx scripts/generate-manifest.ts && cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build && tsx scripts/generate-manifest.ts",
    "build:spa": "tsx scripts/generate-manifest.ts && cross-env NODE_OPTIONS=--max-old-space-size=8192 tsx build.ts && tsx scripts/generate-manifest.ts",
    "dev": "tsx scripts/generate-manifest.ts && vite",
    "generate:manifest": "tsx scripts/generate-manifest.ts",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "typecheck": "tsc",
    "setup": "start SETUP_INSTRUCTIONS.html",
    "setup:guide": "start SETUP_INSTRUCTIONS.html"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
